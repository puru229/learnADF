{
	"name": "MyFirstDataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Movies",
						"type": "DatasetReference"
					},
					"name": "Movies"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "OutputCSV",
						"type": "DatasetReference"
					},
					"name": "MoviesOutput"
				}
			],
			"transformations": [
				{
					"name": "MoviesYear"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movieId as short,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Movies",
				"Movies derive(MovieYear = toInteger(trim(right(title,6),'()')),",
				"          title = toString(left(title,length(title)-6))) ~> MoviesYear",
				"MoviesYear sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> MoviesOutput"
			]
		}
	}
}